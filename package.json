{
    "name": "@davidegit/vue-ui-table",
    "version": "0.2.5",
    "description": "A flexible table component for Vue.js",
    "author": "Davide Vergine",
    "homepage": "https://davidegit.github.io/vue-ui-table/",
    "private": false,
    "main": "dist/vue-ui-table.common.js",
    "files": [
        "dist/*"
    ],
    "scripts": {
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build --dest docs/",
        "build-lib": "vue-cli-service build --target lib --name vue-ui-table src/vue-ui-table.js && cp -a src/assets/scss dist/scss && rm -rf dist/demo.html dist/scss/docs.scss",
        "npm-publish": "sh ./publish.sh",
        "docs": "sh ./build-docs.sh"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/davidegit/vue-ui-table.git"
    },
    "keywords": [
        "vue",
        "vuejs",
        "table",
        "pagination",
        "ui",
        "components"
    ],
    "license": "GPL-3.0-or-later",
    "dependencies": {
        "@fortawesome/fontawesome-free": "^5.12.1",
        "lodash": "^4.17.15"
    },
    "devDependencies": {
        "@babel/polyfill": "^7.4.4",
        "@vue/cli-plugin-babel": "~4.2.0",
        "@vue/cli-plugin-router": "~4.2.0",
        "@vue/cli-service": "~4.2.0",
        "bulma": "^0.8.0",
        "bulma-extensions": "^6.2.7",
        "sass": "^1.25.0",
        "sass-loader": "^8.0.2",
        "vue": "^2.6.11",
        "vue-affix": "^0.5.2",
        "vue-highlightjs": "^1.3.3",
        "vue-router": "^3.1.6",
        "vue-template-compiler": "^2.6.11",
        "axios": "^0.19.2"
    },
    "optionalDependencies": {
        "vue-i18n": "^8.0.0"
    },
    "peerDependencies": {
        "vue": "^2.5.0",
        "axios": "^0.18.0"
    }
}
